  <%= f.hidden_field :owner_id %>

  <div class="group">
    <%= f.label :title, t("activerecord.attributes.blast.title", :default => "Title"), :class => :label %>
    <%= f.text_field :title, :class => 'text_field' %>
    <span class="description">Ex: Job Title</span>
  </div>

  <div class="group">
    <%= f.label :entry_id, t("activerecord.attributes.blast.entry_id", :default => "Fast File Id (in contents)"), :class => :label %>
    <%= f.number_field :entry_id, :class => 'number_field' %>
    <span class="description">Ex: 1</span>
  </div>

  <div class="group">
    <%= f.label :status, t("activerecord.attributes.blast.status", :default => "Status"), :class => :label %>
    <%= f.select("status", {"pending" => "pending", "processing" => "processing", "finalized" => "finalized"}) %>
  </div>
  
  <div class="group">
    <%= f.label :output_id, t("activerecord.attributes.blast.output_id", :default => "Output (Result)"), :class => :label %>
    <% if f.object && f.object.output %>
      <%= link_to f.object.output.description, f.object.output.source.url %>
    <% else %>
      <h3>Nothing</h3>
    <% end %>
  </div>

  <div class="group">
    <%= f.label :program, t("activerecord.attributes.blast.program", :default => "Program"), :class => :label %>
    <%= f.text_field :program, :class => 'text_field' %>
    <span class="description">Ex: tblastn </span>
  </div>

  <div class="group">
    <%= f.label :database, t("activerecord.attributes.blast.database", :default => "Database"), :class => :label %>
    <%= f.text_field :database, :class => 'text_field' %>
    <span class="description">Ex: chromosome</span>
  </div>

  <div class="group">
    <%= f.label :word_size, t("activerecord.attributes.blast.word_size", :default => "Word size"), :class => :label %>
    <%= f.number_field :word_size, :class => 'number_field' %>
    <span class="description">Ex: 11</span>
  </div>

  <div class="group">
    <%= f.label :max_target_sequence, t("activerecord.attributes.blast.max_target_sequence", :default => "Max target sequence"), :class => :label %>
    <%= f.number_field :max_target_sequence, :class => 'number_field' %>
    <span class="description">Ex: 100</span>
  </div>

  <div class="group">
    <%= f.label :expect, t("activerecord.attributes.blast.expect", :default => "Expect"), :class => :label %>
    <%= f.number_field :expect, :class => 'number_field' %>
    <span class="description">Ex: 10</span>
  </div>

  <div class="group">
    <%= f.label :max_matches_range, t("activerecord.attributes.blast.max_matches_range", :default => "Max matches range"), :class => :label %>
    <%= f.number_field :max_matches_range, :class => 'number_field' %>
    <span class="description">Ex: 0</span>
  </div>

  <div class="group">
    <%= f.label :m_scores, t("activerecord.attributes.blast.m_scores", :default => "Match/Mismatch scores"), :class => :label %>
    <%= f.text_field :m_scores, :class => 'text_field' %>
    <span class="description">Ex: 1,-3</span>
  </div>

  <div class="group">
    <%= f.label :gap_costs, t("activerecord.attributes.blast.gap_costs", :default => "Gap costs"), :class => :label %>
    <%= f.text_field :gap_costs, :class => 'text_field' %>
    <span class="description">Ex: 0 0 (Linear)</span>
  </div>
<div class="group navform wat-cf">
  <button class="button" type="submit">
    <%= image_tag("web-app-theme/icons/tick.png", :alt => "#{t("web-app-theme.save", :default => "Save")}") %> <%= t("web-app-theme.save", :default => "Save") %>
  </button>
  <span class="text_button_padding"><%= t("web-app-theme.or", :default => "or") %></span>
	<%= link_to t("web-app-theme.cancel", :default => "Cancel"), blasts_path, :class => "text_button_padding link_button" %>
</div>
